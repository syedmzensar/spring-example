<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
		http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-4.3.xsd">

	<context:annotation-config></context:annotation-config>
	
	<bean id="employeeOne" class="com.zensar.beans.Employee">
		<property name="employeeID" value="179"></property>
		<property name="employeeName" value="Syed"></property>
	</bean>

	<bean id="department" class="com.zensar.beans.Department">
		<property name="departmentName" value="IT"></property>
	</bean>
	
	<bean id="departmentTwo" class="com.zensar.beans.Department">
		<property name="departmentName" value="HR"></property>
	</bean>
	
</beans>
